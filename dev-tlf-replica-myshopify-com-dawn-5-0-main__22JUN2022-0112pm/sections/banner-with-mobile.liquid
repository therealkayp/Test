{{ 'hero-banner.css' | asset_url | stylesheet_tag }}

<div data-v-460a2e77="" class="crop-image-container hero-banner-section {% if template == 'index' %}home-banner{% endif %}{% if section.settings.herobannerclass %} {{section.settings.herobannerclass}} {% endif %} {% if section.settings.check_banner_image %}hero_image_main{% else %}hero_background_main{% endif %}">
    <div data-v-460a2e77="" class="crop-image crop-our-wine {% if section.settings.check_banner_image %}hero_image{% else %}hero_background{% endif %}"></div> 
      <div data-v-460a2e77="" class="transparent-black-box"></div> 
      {% if section.settings.check_image_url == false%}
         <div class="main-banner-heading">

               {% if  section.settings.hero_banner_title != blank  %}
                  {% if section.settings.herobannerclass == "home-third"%}
                     <p aria-label ="{{ section.settings.hero_banner_title}}" data-v-460a2e77="" class="line-one"> {{ section.settings.hero_banner_title}}</p>
                  {% else %}
                     <h1 aria-label ="{{ section.settings.hero_banner_title}}" data-v-460a2e77="" class="line-one title"> {{ section.settings.hero_banner_title}}</h1>
                 {% endif %}
               {% endif %}

               {% if  section.settings.hero_banner_subtitle != blank %} 
                  <h1 aria-label ="{{ section.settings.hero_banner_subtitle }}" data-v-460a2e77="" class="line-one"> {{ section.settings.hero_banner_subtitle}}</h1>
               {% endif %}
               
               {% if  section.settings.hero_banner_description  %} 
                  <div data-v-460a2e77="" class="line-two main-banner-description">
                     {{ section.settings.hero_banner_description}}
                  </div> 
               {% endif %}

               {%- if section.settings.button_label != blank -%}
                <a{% if section.settings.button_link != blank %} href="{{ section.settings.button_link }}"{% endif %} class="button" {% if section.settings.button_link != blank %} aria-label="{{ section.settings.button_alt }}"{% endif %}{% if section.settings.button_link == blank %} aria-disabled="true"{% endif %}>
                  {{ section.settings.button_label | escape }}
                </a>
              {%- endif -%}

         </div>
         {% if section.settings.image_on_hero_background  %} 
            <div class="main-banner-image">
            </div>
         {% endif %}
      {% endif %}


      {% if section.settings.check_image_url == true %}
        <div data-v-460a2e77="" class="line-one svg-image-box">   
           <span data-v-460a2e77="" class="title"> 
              <div class="desktop wow fadeInDown"> {{section.settings.image_url}} </div>
              <div class="mobile" wow fadeInDown> {{section.settings.mb_image_url}} </div>
            </span>
         </div>
      {% endif %}
  
   {% if section.settings.hero_banner_scroll %}
   <div data-v-460a2e77="" class="scroll">
      {{ section.settings.hero_banner_bottom_text }}
      <div data-v-460a2e77="" class="mouse"><img data-v-460a2e77="" src="data:image/svg+xml;base64," alt=""></div> 
      <div data-v-460a2e77="" class="down-arrow"><img data-v-460a2e77="" src="https://cdn.shopify.com/s/files/1/0595/7373/1528/files/arrow-down-sign-to-navigate_1.png?v=1632219706" alt="down arrow"></div>
   </div>
   <script>
      $('.scroll .down-arrow').click(function(e) {
         $('html, body').animate({
            'scrollTop' : $('.ww-slick-carousel-section').position().top - 100
        });
      });
   </script>
  {% endif %}
</div>  
